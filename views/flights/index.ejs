<%- include('../partials/header') %>
<h2>Flight List</h2>
<table>
  <thread>
     <tr> 
      <th>Arline</th>
      <th>Airport</th>
      <th>Flight Number</th>
      <th>Date of departure</th>
     </tr> 
  </thread>
<tbody>
  <% flight.forEach(function(f) { %>
      <tr>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <td><%= f.flightNo %></td>
          <td><%= f.departs %></td>
      </tr>
  <% }); %> 
</tbody>
</table>
<%- include('../partials/footer') %>
